DAY 2
database of amazon
create database amazon;
show databases;
drop database amazon;
create database amazon;
use amazon;
create table customers(
id int not null,
name varchar(30) not null,
age int not null,
address char(30) not null,
primary key (ID)); 
DESC customers;
CREATE TABLE ROHIT(NAME VARCHAR(10));
DROP TABLE ROHIT;
INSERT INTO  CUSTOMERS(ID,NAME,AGE,ADDRESS) VALUES (01,'ROHIT',23,'TINDIVANAM');
INSERT INTO CUSTOMERS(ID,NAME,AGE,ADDRESS) VALUES (02,'SUKANYA',26,'CHENNAI');
INSERT INTO CUSTOMERS(ID,NAME,AGE,ADDRESS) VALUES (03,'SATHISH',29,'BANGALORE');
INSERT INTO CUSTOMERS(ID,NAME,AGE,ADDRESS) VALUES (04,'SEKAR',62,'TINDIVANAM');
DESC CUSTOMERS;
SELECT*FROM CUSTOMERS;
CREATE TABLE SELLERS(SELLID INT NOT NULL,NAME VARCHAR(20) NOT NULL,AGE INT NOT NULL,ADDRESS CHAR(30) NOT NULL,PRIMARY KEY(SELLID));
INSERT INTO  SELLERS(SELLID,NAME,AGE,ADDRESS)VALUES(001,'NOKIA',20,'KOREA');
INSERT INTO SELLERS(SELLID,NAME,AGE,ADDRESS)VALUES(002,'APPLE',30,'CALIFORNIA');
INSERT INTO SELLERS(SELLID,NAME,AGE,ADDRESS)VALUES(003,'XIOMI',10,'CHINA');
SELECT*FROM SELLERS;
SELECT ID,NAME FROM CUSTOMERS;
SELECT SELLID,NAME FROM SELLERS;
SELECT ID FROM CUSTOMERS WHERE AGE >=26;
SELECT NAME FROM SELLERS WHERE AGE >10;
SELECT ID,AGE,ADDRESS FROM CUSTOMERS WHERE NAME='ROHIT';
SELECT ID,NAME FROM CUSTOMERS WHERE AGE>=26 AND ADDRESS!='TINDIVANAM';
SELECT ID NAME FROM CUSTOMERS

